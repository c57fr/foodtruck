<div class="row my-2">
    <h2>{{ action }} une adresse</h2>
</div>
<div class="container">
<div class="row my-2">
<div class="card">
    {{ form_start(form) }}
        {{ form_row(form.ville) }}
        {{ form_row(form.image) }}
        {{ form_row(form.adresse) }}
        {{ form_row(form.jour) }}
        {#{{ form_row(form.debut) }}#}
        {#{{ form_row(form.fin) }}#}
        {{form_rest(form)}}
        
        {{ form_errors(form) }}
        <button type="submit" class="btn btn-success">{{ action }}</button>
    {{ form_end(form) }}
</div>
</div>
</div>